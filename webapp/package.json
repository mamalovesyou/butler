{
  "name": "@butler/dashboard",
  "author": "Matthieu Berger <matthieu@heybutler.io>",
  "version": "0.0.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/butlerhq/butler.git"
  },
  "bugs": {
    "url": "https://github.com/butlerhq/butler/issues"
  },
  "scripts": {
    "env": "dotenv -e .env.local",
    "build": "yarn run clean-dist && webpack --config=config/webpack/prod.js",
    "start": "yarn run start-dev",
    "start-dev": "webpack serve --config=config/webpack/dev.js",
    "clean-dist": "rimraf dist/*",
    "legacy:format": "prettier --config .prettierrc './src/**/*.{ts,tsx,js,jsx}' --write",
    "legacy:lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "lint:fix": "yarn lint --fix",
    "lint:quiet": "yarn lint --quiet",
    "gen:api": "swagger-typescript-api --axios --clean-output -p config/openapi/api.swagger.json -o ./src/api/gen -n index.ts"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.0",
    "@mui/material": "^5.2.1",
    "@reduxjs/toolkit": "^1.5.1",
    "axios": "^0.24.0",
    "formik": "^2.2.9",
    "fs-extra": "^9.0.1",
    "history": "^5.1.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dev-utils": "^11.0.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.0",
    "react-refresh": "^0.8.3",
    "react-router-dom": "6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-first-history": "^5.0.3",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "semver": "7.3.2",
    "simplebar": "^5.3.6",
    "simplebar-react": "^2.3.6",
    "yup": "^0.32.11",
    "yup-env": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@svgr/webpack": "^6.0.0",
    "@types/node": "^16.11.10",
    "@types/prop-types": "^15.7.4",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.7",
    "@types/yup": "^0.29.13",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "dotenv-cli": "^4.1.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^8.3.0",
    "eslint-config-airbnb": "^19.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.0.1",
    "prettier": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "swagger-typescript-api": "^9.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0"
  }
}
