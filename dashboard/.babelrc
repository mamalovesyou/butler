{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "react-hot-loader/babel"
    ],
    "env": {
        "production": {
            "presets": [
                "minify"
            ]
        }
    }
}